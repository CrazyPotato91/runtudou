 \"line\":\n        new Line(data);\n        break;\n      case \"pie\":\n        new Pie(data);\n        break;\n      case \"scatter\":\n        new Scatter(data);\n        break;\n      case \"stackedbar\":\n        new StackedBar(data);\n        break;\n    }\n  }\n};\n</script>\n\n<style lang=\"stylus\">\n.vuepress-roughviz-container\n  margin 30px 0\n  text-align center\n  .vuepress-roughviz\n    overflow scroll\n    display inline-block\n</style>\n"]}]}