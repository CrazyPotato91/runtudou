\n  Line,\n  Pie,\n  Scatter,\n  StackedBar\n} from \"rough-viz/dist/roughviz.min.js\";\n\nexport default {\n  name: \"Roughviz\",\n  props: {\n    id: {\n      type: String,\n      required: true\n    },\n    chart: {\n      type: String,\n      required: true\n    },\n    code: {\n      type: String,\n      required: true\n    }\n  },\n  mounted() {\n    let data = JSON.parse(this.code);\n    data.element = `#${this.id}`;\n\n    switch (this.chart) {\n      case \"bar\":\n        new Bar(data);\n        break;\n      case \"barh\":\n        new BarH(data);\n        break;\n      case \"donut\":\n        new Donut(data);\n        break;\n      case \"line\":\n        new Line(data);\n        break;\n      case \"pie\":\n        new Pie(data);\n        break;\n      case \"scatter\":\n        new Scatter(data);\n        break;\n      case \"stackedbar\":\n        new StackedBar(data);\n        break;\n    }\n  }\n};\n</script>\n\n<style lang=\"stylus\">\n.vuepress-roughviz-container\n  margin 30px 0\n  text-align center\n  .vuepress-roughviz\n    overflow scroll\n    display inline-block\n</style>\n"],"sourceRoot":"packages/plugins/roughviz/lib"}]}