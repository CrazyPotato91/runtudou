,CAAA,YADA,EAEA,YAFA,EAGA,kBAAA,CAAA,YAHA,EAIA,YAAA,CAAA,YAJA,EAKA,CALA,EAMA,IANA,CAMA,UAAA,KAAA;AAAA,aAAA,KAAA,KAAA,SAAA;AAAA,KANA,CAAA;AAQA,QAAA,iBAAA,GACA,kBAAA,CAAA,iBAAA,IAAA,YAAA,CAAA,iBADA;;AAGA,QAAA,IAAA,CAAA,IAAA,KAAA,MAAA,EAAA;AACA,aAAA,CACA,IADA,EAEA,cAAA,CAAA,CAAA,EAAA,IAAA,CAAA,QAAA,EAAA,IAAA,CAAA,QAAA,EAAA,MAAA,EAAA,QAAA,CAFA,CAAA;AAIA,KALA,MAKA,IACA,CAAA,MAAA,IAAA,iBAAA,KACA,IAAA,CAAA,OADA,IAEA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAHA,EAIA;AACA,UAAA,QAAA,GAAA,YAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA,aAAA,CAAA,IAAA,EAAA,cAAA,CAAA,CAAA,EAAA,QAAA,EAAA,IAAA,CAAA,IAAA,EAAA,MAAA,EAAA,QAAA,CAAA,CAAA;AACA,KAPA,MAOA;AACA,aAAA,IAAA;AACA;AACA;AAhEA,CAAA;;AAmEA,SAAA,UAAA,CAAA,CAAA,EAAA,EAAA,EAAA,IAAA,EAAA,MAAA,EAAA,KAAA,EAAA;AACA,MAAA,SAAA,GAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,EAAA,EAAA,EADA;AAEA,MAAA,WAAA,EAAA,EAFA;AAGA,MAAA,gBAAA,EAAA;AAHA,KADA;AAMA,aAAA;AACA,MAAA,MAAA,EAAA,MADA;AAEA,sBAAA;AAFA;AANA,GAAA;;AAYA,MAAA,KAAA,GAAA,CAAA,EAAA;AACA,IAAA,SAAA,CAAA,KAAA,GAAA;AACA,sBAAA,KAAA,GAAA;AADA,KAAA;AAGA;;AAEA,SAAA,CAAA,CAAA,YAAA,EAAA,SAAA,EAAA,IAAA,CAAA;AACA;;AAEA,SAAA,cAAA,CAAA,CAAA,EAAA,QAAA,EAAA,IAAA,EAAA,KAAA,EAAA,QAAA,EAAA;AAAA,MAAA,KAAA,uEAAA,CAAA;AACA,MAAA,CAAA,QAAA,IAAA,KAAA,GAAA,QAAA,EAAA,OAAA,IAAA;AACA,SAAA,CAAA,CACA,IADA,EAEA;AAAA,aAAA;AAAA,GAFA,EAGA,QAAA,CAAA,GAAA,CAAA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,GAAA,QAAA,CAAA,KAAA,EAAA,IAAA,GAAA,GAAA,GAAA,CAAA,CAAA,IAAA,CAAA;AACA,WAAA,CAAA,CAAA,IAAA,EAAA;AAAA,eAAA;AAAA,KAAA,EAAA,CACA,UAAA,CAAA,CAAA,EAAA,IAAA,GAAA,GAAA,GAAA,CAAA,CAAA,IAAA,EAAA,CAAA,CAAA,KAAA,EAAA,MAAA,EAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CADA,EAEA,cAAA,CAAA,CAAA,EAAA,CAAA,CAAA,QAAA,EAAA,IAAA,EAAA,KAAA,EAAA,QAAA,EAAA,KAAA,GAAA,CAAA,CAFA,CAAA,CAAA;AAIA,GANA,CAHA,CAAA;AAWA;;AAEA,SAAA,cAAA,CAAA,CAAA,EAAA,EAAA,EAAA,IAAA,EAAA;AACA,SAAA,CAAA,CACA,GADA,EAEA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,MAAA,EAAA,QAFA;AAGA,MAAA,GAAA,EAAA;AAHA,KADA;AAMA,aAAA;AACA,sBAAA;AADA;AANA,GAFA,EAYA,CAAA,IAAA,EAAA,CAAA,CAAA,cAAA,CAAA,CAZA,CAAA;AAcA","sourcesContent":["<script>\nimport { isActive, hashRE, groupHeaders } from \"@theme/utils/utils\";\n\nexport default {\n  functional: true,\n\n  props: {\n    item: {\n      type: Object,\n      default: () => {}\n    },\n    sidebarDepth: {\n      type: Number,\n      default: undefined\n    }\n  },\n\n  render(\n    h,\n    {\n      parent: { $page, $site, $route, $themeConfig, $themeLocaleConfig },\n      props: { item, sidebarDepth }\n    }\n  ) {\n    // use custom active class matching logic\n    // due to edge case of paths ending with / + hash\n    const selfActive = isActive($route, item.path);\n    // for sidebar: auto pages, a hash link should be active if one of its child\n    // matches\n    const active =\n      item.type === \"auto\"\n        ? selfActive ||\n          item.children.some((c) =>\n            isActive($route, item.basePath + \"#\" + c.slug)\n          )\n        : selfActive;\n    const link =\n      item.type === \"external\"\n        ? renderExternal(h, item.path, item.title || item.path)\n        : renderLink(h, item.path, item.title || item.path, active);\n\n    const maxDepth = [\n      $page.frontmatter.sidebarDepth,\n      sidebarDepth,\n      $themeLocaleConfig.sidebarDepth,\n      $themeConfig.sidebarDepth,\n      1\n    ].find((depth) => depth !== undefined);\n\n    const displayAllHeaders =\n      $themeLocaleConfig.displayAllHeaders || $themeConfig.displayAllHeaders;\n\n    if (item.type === \"auto\") {\n      return [\n        link,\n        renderChildren(h, item.children, item.basePath, $route, maxDepth)\n      ];\n    } else if (\n      (active || displayAllHeaders) &&\n      item.headers &&\n      !hashRE.test(item.path)\n    ) {\n      const children = groupHeaders(item.headers);\n      return [link, renderChildren(h, children, item.path, $route, maxDepth)];\n    } else {\n      return link;\n    }\n  }\n};\n\nfunction renderLink(h, to, text, active, level) {\n  const component = {\n    props: {\n      to,\n      activeClass: \"\",\n      exactActiveClass: \"\"\n    },\n    class: {\n      active,\n      \"sidebar-link\": true\n    }\n  };\n\n  if (level > 2) {\n    component.style = {\n      \"padding-left\": level + \"rem\"\n    };\n  }\n\n  return h(\"RouterLink\", component, text);\n}\n\nfunction renderChildren(h, children, path, route, maxDepth, depth = 1) {\n  if (!children || depth > maxDepth) return null;\n  return h(\n    \"ul\",\n    { class: \"sidebar-sub-headers\" },\n    children.map((c) => {\n      const active = isActive(route, path + \"#\" + c.slug);\n      return h(\"li\", { class: \"sidebar-sub-header\" }, [\n        renderLink(h, path + \"#\" + c.slug, c.title, active, c.level - 1),\n        renderChildren(h, c.children, path, route, maxDepth, depth + 1)\n      ]);\n    })\n  );\n}\n\nfunction renderExternal(h, to, text) {\n  return h(\n    \"a\",\n    {\n      attrs: {\n        href: to,\n        target: \"_blank\",\n        rel: \"noopener noreferrer\"\n      },\n      class: {\n        \"sidebar-link\": true\n      }\n    },\n    [text, h(\"OutboundLink\")]\n  );\n}\n</script>\n\n<style lang=\"stylus\">\n.sidebar .sidebar-sub-headers\n  padding-left 1rem\n  font-size 0.95em\n\na.sidebar-link\n  font-size 1em\n  font-weight 400\n  display inline-block\n  color var(--text-color)\n  background var(--bg-color)\n  border-left 0.25rem solid transparent\n  padding 0.35rem 1rem 0.35rem 1.25rem\n  line-height 1.4\n  width: 100%\n  box-sizing: border-box\n  &:hover\n    color var(--accent-color)\n  &.active\n    font-weight 600\n    color var(--accent-color)\n    border-left-color var(--accent-color)\n  .sidebar-group &\n    padding-left 2rem\n  .sidebar-sub-headers &\n    padding-top 0.25rem\n    padding-bottom 0.25rem\n    border-left none\n    &.active\n      font-weight 500\n</style>\n"],"sourceRoot":"packages/theme-gungnir/components"}]}